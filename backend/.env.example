# Copy this file to .env and fill in values.
# Never commit your real keys.

GEMINI_API_KEY=YOUR_KEY_HERE
# Optional: override model; defaults to gemini-pro (text chat)
# Examples: gemini-pro, gemini-1.5-pro, gemini-1.5-flash
GEMINI_MODEL_ID=gemini-pro
# Use new Google GenAI SDK instead of deprecated google.generativeai
USE_GOOGLE_GENAI=false
